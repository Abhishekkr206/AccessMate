{
  "manifest_version": 3,
  "name": "Content Script Demo",
  "version": "1.0",
  "description": "Make any website more accessible for disabled users.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": [],
  "action": {
    "default_icon": {
    "16": "icons/AccessMate.png",
    "48": "icons/AccessMate.png",
    "128": "icons/AccessMate.png"
  }
  }
}
